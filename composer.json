{
    "name": "laranex/laralog-client",
    "description": "Client package for Laralog Server",
    "keywords": [
        "laranex",
        "laralog-client",
        "laralog",
        "logging",
        "laravel"
    ],
    "homepage": "https://github.com/laranex/laralog-client",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Nay Thu Khant",
            "email": "naythukhant644@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0.2"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Laranex\\LaralogClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laranex\\LaralogClient\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laranex\\LaralogClient\\LaralogClientServiceProvider"
            ]
        }
    }
}
